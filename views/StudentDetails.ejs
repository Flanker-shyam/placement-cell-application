<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>User Data</title>
</head>

<body>
  <h1>User Data</h1>
  <ul>
    <% users.forEach(user => { %>
      <li>
        <h2><%= user.name %></h2>
        <p>Registration Number: <%= user.regNo %></p>
        <p>Email: <%= user.email %></p>
      <!-- Comment -->
      </li>
    <% }); %>
  </ul>
</body>

</html>
